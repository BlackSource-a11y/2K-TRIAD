    local Button = PlayerTab:CreateButton({
        Name = "VISUAL VIRUS  [BSK]", 
        Callback = function()
            -- THIS SCRIPT MADE BY Louie
            local player = game.Players.LocalPlayer
            local gui = player:WaitForChild("PlayerGui")

            -- Play loud warning sound
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://138211359" -- loud alarm sound
            sound.Volume = 100
            sound.Looped = true
            sound.Parent = workspace
            sound:Play()

            -- Function to create one fake virus window
            local function createWindow()
                local ScreenGui = Instance.new("ScreenGui")
                local Frame = Instance.new("Frame")
                local TextLabel = Instance.new("TextLabel")

                ScreenGui.Parent = gui

                Frame.Parent = ScreenGui
                Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                Frame.Position = UDim2.new(math.random(), 0, math.random(), 0)
                Frame.Size = UDim2.new(0, 250, 0, 120)

                TextLabel.Parent = Frame
                TextLabel.Size = UDim2.new(1, 0, 1, 0)
                TextLabel.BackgroundTransparency = 1
                TextLabel.Text = "BERSERK ONTOP BOBO MO TOL!!"
                TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
                TextLabel.TextScaled = true
            end

            -- Spawn 1000 frames very fast (spreading effect)
            for i = 1, 1000 do
                wait(0.01)
                createWindow()
            end
        end,
    })


