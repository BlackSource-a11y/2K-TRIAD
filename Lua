local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

    local Window = Rayfield:CreateWindow({
    Name = "BERSERK",                  -- Name of your window
    Icon = 0,                           -- Icon for the topbar (0 means no icon)
    LoadingTitle = "BERSERK ONTOP!!",          -- Title displayed while loading
    LoadingSubtitle = "by BERSERK",   -- Subtitle displayed while loading
    ShowText = "Rayfield",              -- For mobile users, text to unhide Rayfield
    Theme = "Default",                  -- Theme for the UI
    ToggleUIKeybind = "K",              -- Keybind to toggle UI visibility
    DisableRayfieldPrompts = false,     -- Disable Rayfield's prompts
    DisableBuildWarnings = false,       -- Disable build warnings
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil,                -- Custom folder for saving configurations
        FileName = "Big Hub"             -- File name for saving
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    

    })

    Rayfield:Notify({
    Title = "2K TRIAD",
    Content = "Thankyou For Using 2K TRIAD Script",
    Duration = 5,
    Image = nil,
    })

    local PlayerTab = Window:CreateTab("BERSERK MAIN", 4483362458) 
local Section =PlayerTab:CreateSection("BERKSERK MAIN CHEAT")
    local AimbotButton = PlayerTab:CreateButton({
    Name = "AIMBOT  [BSK]",
    Callback = function()
        local Aimbot = loadstring(game:HttpGet("https://raw.githubusercontent.com/Exunys/Aimbot-V3/main/src/Aimbot.lua"))()Aimbot.Load()
    end,
    })

    local BigHeadButton = PlayerTab:CreateButton({
    Name = "BIG HEAD  [BSK]",
    Callback = function()
        _G.HeadSize = 4 _G.Disabled = true game:GetService('RunService').RenderStepped:connect(function() if _G.Disabled then for i,v in next, game:GetService('Players'):GetPlayers() do if v.Name ~= game:GetService('Players').LocalPlayer.Name then pcall(function() v.Character.Head.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize) v.Character.Head.Transparency = 0.4 v.Character.Head.BrickColor = BrickColor.new("Red") v.Character.Head.Material = "Neon" v.Character.Head.CanCollide = false v.Character.Head.Massless = true end) end end end end)

    end,
    })

    local Button = PlayerTab:CreateButton({
    Name = "ESP  [BSK]",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/bnsshiva8-art/Esp/f563fd7c275b80dd67cf6410b8ebe324ff39d582/Hub"))()
    end,
    })

    local Button = PlayerTab:CreateButton({
    Name = "TP TOOLS  [BSK]",
    Callback = function()
    loadstring(game:HttpGet("https://pastebin.com/raw/CnrzahUZ"))()
    end,
    })


    local Button = PlayerTab:CreateButton({
    Name = "LOOP BRING  [BSK]", 
    Callback = function()
    loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Bring-All-Client-Sided-Universal-36570"))()
    end,
    })

    local Button = PlayerTab:CreateButton({
    Name = "TOUCH FLING  [BSK]",
    Callback = function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/bnsshiva8-art/FLing/54ef209d2dbbef7361792625debfa1a68b3c9a28/Touch"))()
    end,
    })

