local SpawnerTab = Window:CreateTab("Spawner", 4483362458) -- Title, Image

local Section = SpawnerTab:CreateSection("SPAWN GUN")

local Button = SpawnerTab:CreateButton({
    Name = "ALL CITY SP  [BSK]",
    Callback = function()
        local args = {
            [1] = "Silenced Pistol",
            [2] = -1000000
        }

        game:GetService("ReplicatedStorage").GunShop.Events.Purchase:FireServer("Silenced Pistol", -10000)
    end,
})

